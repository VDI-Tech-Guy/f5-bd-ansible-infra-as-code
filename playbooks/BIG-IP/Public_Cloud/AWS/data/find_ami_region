aws ec2 describe-images --owners 679593333241 --filters "Name=name,Values=*BIGIP*PAYG*" --region us-east-2


aws ec2 describe-images \
  --region us-east-2 \
  --owners 679593333241 \
  --filters "Name=name,Values=*BIGIP*PAYG*" \
  --query "Images[*].{Name:Name,ImageId:ImageId}" \
  --output table


